<%- include('./partials/header') %>
    <h1>Landing Page</h1>
    <% upcoming.forEach(g => { %>
        <div class="upcoming-index">
            <img src="<%= g.photo %>" alt="No photo available">
                <div id="upcoming-title">
                    <a href="/games/<%= g._id %>" id='upcoming-title'><p><%= g.name %></p></a>
                </div>
                </a>
        </div>
        
    <% }) %>
</body>
</html>